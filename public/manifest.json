{
  "manifest_version": 3,
  "name": "Ajimaru",
  "version": "0.0.1",
  "action": {
    "default_icon": "icon32_disable.png",
    "default_title": "あじまる",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "description": "配信開始時刻まで瞬時にジャンプできるChrome拡張",
  "options_page": "options.html",
  "permissions": [
    "tabs"
  ]
}
